.cntloading {
  padding: 50rpx 0;
  text-align: center;
  color: #888;
  font-size: 26rpx;
}

.tab {
  display: flex;
  border-bottom: 1rpx solid rgba(200, 200, 200, 0.5);
  background: rgba(240, 240, 240, 0.8);
  height: 80rpx;
  line-height: 80rpx;
}

.tab .tab-item {
  text-align: center;
  flex: auto;
  word-break: keep-all;
  font-size: 30rpx;
}

.tab .tbi-cur .tbi-text {
  color: #d33a31;
  margin-bottom: -1rpx;
  border-bottom: 4rpx solid #d33a31;
}

.tab .tab-item .tbi-text {
  display: inline-block;
  padding: 0 0.3em;
  vertical-align: top;
  line-height: 76rpx;
  height: 77rpx;
}

.st_title {
  overflow: hidden;
  margin: 10rpx 15rpx 0 15rpx;
  padding: 10rpx;
  padding-left: 0;
  line-height: 36rpx;
  font-size: 28rpx;
}

.st_title image {
  width: 36rpx;
  height: auto;
  margin-right: 0.3em;
  vertical-align: middle;
  font-size: 28rpx;
}

.flex-boxlist {
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
}

.tl_cnt {
  width: 231.3rpx;
  margin: 15rpx 0 15rpx 15rpx;
  box-sizing: border-box;
  line-height: 1.2;
}

.tl_cnt text {
  font-size: 26rpx;
  color: #666;
}

.tl_cnt .cover .music_cover, .tl_cnt .cover .mv_cover, .cover .album_cover {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.cover {
  position: relative;
  height: 0;
  padding-top: 100%;
  overflow: hidden;
  margin-bottom: 10rpx;
}

.img_playcount {
  position: absolute;
  right: 0;
  top: 0;
  color: #fff;
  z-index: 1;
  font-size: 24rpx;
  padding: 8rpx 15rpx 8rpx 30rpx;
  background-image: linear-gradient(90deg, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.2));
  max-width: 70%;
  text-align: right;
  min-width: 30%;
}

.img_playcount image {
  vertical-align: baseline;
  margin-right: 10rpx;
  position: relative;
  width: 24rpx;
  height: 24rpx;
}

.tl_cnt .name {
  line-height: 1.4;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.st_title .rbtn {
  float: right;
  color: #999;
  font-size: 24rpx;
}

.tl_info {
  font-size: 26rpx;
  line-height: 1.8em;
}

.tl_info view {
  word-break: keep-all;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.tl_info .tli_des {
  color: #888;
  font-size: 24rpx;
}

.flex-two .tl_cnt {
  width: 352.5rpx;
}

.mvs .cover {
  padding-top: 56%;
}

.mv_cover {
  background-size: 100% auto;
  background-position: center center;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}

/* 歌单 */
.listheader {
  margin: 25rpx 15rpx 10rpx;
  overflow: hidden;
}

@keyframes circle {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}


@keyframes cnloading {
  0% {
    transform: translateY(0rpx);
  }

  50% {
    transform: translateY(20rpx);
  }

  100% {
    transform: translateY(0rpx);
  }
}

.cntloading text {
  width: 5rpx;
  height: 30rpx;
  background-color: #d33a31;
  position: relative;
  display: inline-block;
  margin-right: 6rpx;
  border-radius: 3rpx;
  animation: cnloading 0.9s linear infinite;
  transform-origin: center bottom;
}

.cntloading view {
  display: inline-block;
  vertical-align: baseline;
  height: 30rpx;
  overflow: hidden;
  margin-left: 10rpx;
}

.cntloading .cl2 {
  animation-delay: -.6s;
}

.cntloading .cl3 {
  animation-delay: -.3s;
}

/* 排行榜 */
.album_cover {
  background-size: cover;
  background-image: url(data:image/png;base64,);
}

.fl-image image{
  width: 100rpx;
  height: 100rpx;
  vertical-align: middle;
  position: relative;
  z-index: 1;

}

.fl-image {
  position: relative;
  padding: 10rpx 0;
  margin: 0 20rpx;
}

.tl_info view {
  word-break: keep-all;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.flexlist .flexlist {
  flex: 1;
  padding: 10rpx 10rpx 10rpx 0;
  margin: 0;
  align-items: center;
  margin-left: 0;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 1.5;
  position: relative;
  border-bottom: 1rpx solid #e1e2e3;
}

